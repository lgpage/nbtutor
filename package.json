{
  "name": "nbtutor",
  "version": "2.0.1-dev",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "e2e": "ng e2e",
    "lint:ui": "ng lint",
    "lint:python": "python -m mypy nbtutor && python -m flake8 nbtutor setup.py",
    "lint:all": "npm run lint:ui && npm run lint:python",
    "build:ui": "node pre-build.js && ng build --prod --output-hashing none && node post-build.js",
    "build:ui:stats": "ng build --prod --stats-json",
    "test:ui": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test:python": "python -m pytest --cov --cov-report=term-missing -vv tests nbtutor",
    "test:all": "npm run test:ui && npm run test:python",
    "test:before-push": "npm run lint:all && npm run build:ui && npm run test:all",
    "extension:enable": "jupyter nbextension enable nbtutor --py --sys-prefix",
    "extension:install": "jupyter nbextension install --overwrite --sys-prefix --py nbtutor",
    "extension:update": "npm run build:ui && npm run extension:install && npm run extension:enable",
    "docs:build": "sphinx-build -b html docs dist/docs",
    "analyze:stats": "webpack-bundle-analyzer ./dist/nbtutor/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/cdk": "~10.2.7",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/material": "~10.2.7",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/platform-browser": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@ngrx/effects": "~10.1.2",
    "@ngrx/entity": "~10.1.2",
    "@ngrx/store": "~10.1.2",
    "core-js": "~3.8.2",
    "jsplumb": "~2.15.5",
    "rxjs": "~6.6.3",
    "store": "~2.0.12",
    "tslib": "^2.1.0",
    "uuid": "~7.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "^10.2.1",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@angular/localize": "^10.2.4",
    "@ngrx/schematics": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "@types/codemirror": "~0.0.96",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.5.5",
    "@types/node": "^13.11.1",
    "@types/uuid": "^7.0.2",
    "codelyzer": "^6.0.1",
    "codemirror": "^5.55.0",
    "concat": "^1.0.3",
    "fibers": "^5.0.0",
    "fs-extra": "^9.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "jquery": "^3.5.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-mocks": "^11.3.1",
    "ng-packagr": "^10.1.2",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.34.7",
    "sass": "^1.32.2",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "~3.6.1"
  }
}
