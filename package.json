{
  "name": "nbtutor",
  "version": "2.0.1-dev",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "e2e": "ng e2e",
    "lint:ui": "ng lint",
    "lint:python": "python -m mypy nbtutor && python -m flake8 nbtutor setup.py",
    "lint:all": "npm run lint:ui && npm run lint:python",
    "build:ui": "node pre-build.js && ng build --prod --output-hashing none && node post-build.js",
    "build:ui:stats": "ng build --prod --stats-json",
    "test:ui": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test:python": "python -m pytest --cov --cov-report=term-missing -vv tests nbtutor",
    "test:all": "npm run test:ui && npm run test:python",
    "test:before-push": "npm run lint:all && npm run build:ui && npm run test:all",
    "extension:enable": "jupyter nbextension enable nbtutor --py --sys-prefix",
    "extension:install": "jupyter nbextension install --overwrite --sys-prefix --py nbtutor",
    "extension:update": "npm run build:ui && npm run extension:install && npm run extension:enable",
    "docs:build": "sphinx-build -b html docs dist/docs",
    "analyze:stats": "webpack-bundle-analyzer ./dist/nbtutor/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "~11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@ngrx/effects": "~11.1.1",
    "@ngrx/entity": "~11.1.1",
    "@ngrx/store": "~11.1.1",
    "jsplumb": "^2.15.6",
    "rxjs": "^6.6.7",
    "store": "~2.0.12",
    "tslib": "^2.0.0",
    "uuid": "~7.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.14",
    "@angular-eslint/builder": "4.3.0",
    "@angular-eslint/eslint-plugin": "4.3.0",
    "@angular-eslint/eslint-plugin-template": "4.3.0",
    "@angular-eslint/schematics": "4.3.0",
    "@angular-eslint/template-parser": "4.3.0",
    "@angular/cli": "^11.2.14",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@angular/localize": "^11.2.14",
    "@ngrx/schematics": "^11.1.1",
    "@ngrx/store-devtools": "^11.1.1",
    "@types/codemirror": "~0.0.96",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.5.5",
    "@types/node": "^13.11.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codemirror": "^5.62.2",
    "concat": "^1.0.3",
    "eslint": "^7.6.0",
    "eslint-plugin-rxjs": "^3.3.5",
    "fibers": "^5.0.0",
    "fs-extra": "^9.0.0",
    "html-webpack-plugin": "^4.5.2",
    "jasmine-core": "~3.8.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jquery": "^3.6.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^11.3.1",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.34.7",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "~3.6.1"
  }
}
